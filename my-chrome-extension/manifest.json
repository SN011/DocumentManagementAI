{
    "manifest_version": 3,
    "name": "Document Manager",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "doc-mgr-icon.png",
        "48": "doc-mgr-icon.png",
        "128": "doc-mgr-icon.png"
      }
    }
  }
  